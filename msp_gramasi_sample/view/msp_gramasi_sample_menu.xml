<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="action_marel_in_gramasi_sample" model="ir.actions.act_window">
            <field name="name">Gramasi Sample</field>
            <field name="res_model">sample.gramasi</field>
            <field name="view_mode">list,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
					Click to add Sample
				</p>
            </field>
        </record>
        <record id="action_marel_in_permintaan_benang_sample" model="ir.actions.act_window">
            <field name="name">Permintaan benang Sample</field>
            <field name="res_model">marelin.permintaan.benang.sample</field>
            <field name="view_mode">list,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
					Click to add Permintaan Benang
				</p>
            </field>
        </record>
        <menuitem id="marel_in_gramasi_sample_0"
			name="Data Sample"
            web_icon="marel_in_gramasi_sample,static/description/sample.png"
			sequence="100"/>
        <menuitem id="marel_in_gramasi_sample_1"
			name="Sample"
			parent="marel_in_gramasi_sample_0"
			sequence="30"/>
        <!-- sub menu -->
        <menuitem id="menu_marel_in_gramasi_sample"
			name="Data Sample Gramasi"
			parent="marel_in_gramasi_sample_1"
			action="action_marel_in_gramasi_sample"
			sequence="35"/>
        <!-- sub menu -->
        <menuitem id="menu_marel_in_permintaan_benang_sample"
			name="Data Permintaan Benang"
			parent="marel_in_gramasi_sample_1"
			action="action_marel_in_permintaan_benang_sample"
			sequence="35"/>
        <!-- <menuitem id="menu_marel_in_sample_gramasi_product_list"
			name="Data Sample Gramasi Line"
			parent="marel_in_gramasi_sample_1"
			action="action_marel_in_sample_gramasi_product_list"
			sequence="35"/> -->
    </data>
</odoo>